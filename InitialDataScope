         SmartCarDataGenerator obj = new SmartCarDataGenerator();
       
         for(int i=0; i<1000; i++){  
         System.out.println(obj.getRandomItem(author, system, subsystem, methodology, driverBehaviorDetection, variable_car_condition, speedMode));
         }
      }
  
  public String getRandomItem(List<String> author, List<String> system, List<String> subsystem, List<String> methodology, List<String> driverBehaviorDetection, List<String> variable_car_condition, List<String> speedMode){
      Random rand = new Random();
      return author.get(rand.nextInt(author.size()))+ ", " + system.get(rand.nextInt(system.size()))+ ", " + subsystem.get(rand.nextInt(subsystem.size()))+ ", " + methodology.get(rand.nextInt(methodology.size()))+ ", " + driverBehaviorDetection.get(rand.nextInt(driverBehaviorDetection.size()))+ ", " + variable_car_condition.get(rand.nextInt(variable_car_condition.size()))+ ", " + speedMode.get(rand.nextInt(speedMode.size())) ;
      
  }

}
